<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.theband.benno.probeplaner.treetable.LinesTablePresenter">
   <children>
      <TreeTableView fx:id="treeTable" editable="false" tableMenuButtonVisible="true">
        <columns>
          <TreeTableColumn fx:id="name" editable="true" prefWidth="121.0" sortable="false" text="" />
        </columns>
         <contextMenu>
            <ContextMenu>
              <items>
                  <MenuItem mnemonicParsing="false" onAction="#createScene" onMenuValidation="#isCreateSceneVisible" text="%createScene" />
                <MenuItem onAction="#createRehearsal" text="%createRehearsal" />
                  <MenuItem onAction="#delete" text="%delete" />
              </items>
            </ContextMenu>
         </contextMenu>
      </TreeTableView>
   </children>
</VBox>
